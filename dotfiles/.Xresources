! Gruber-Darker
URxvt*foreground: #e4e4ef
URxvt*background: #181818
URxvt*color0:  #2E3436
URxvt*color1:  #a40000
URxvt*color2:  #4E9A06
URxvt*color3:  #C4A000
URxvt*color4:  #3465A4
URxvt*color5:  #75507B
URxvt*color6:  #ce5c00
URxvt*color7:  #babdb9
URxvt*color8:  #555753
URxvt*color9:  #EF2929
URxvt*color10: #8AE234
URxvt*color11: #FCE94F
URxvt*color12: #729FCF
URxvt*color13: #AD7FA8
URxvt*color14: #fcaf3e
URxvt*color15: #EEEEEC

!! URxvt
URxvt*scrollstyle:  plain
!URxvt*buffered: false

URxvt*termName: xterm-256color

!URxvt.font: xft:Ubuntu Mono Nerd Font Mono:style=Regular:style=Medium:size=25:antialias=true:hinting=true,\
!xft:Unifont:style=Medium:size=23
!! Bolt
!URxvt.boldFont: xft:Ubuntu Mono Nerd Font Mono:style=Bold:size=25:antialias=true:hinting=true,\
!xft:Unifont:style=Medium:size=23
!! Italic
!URxvt.italicFont: xft:Ubuntu Mono Nerd Font Mono:style=Light,Regular:size=25:antialias=true:hinting=true,\
!xft:Unifont:style=Medium:size=23

URxvt.font: xft:Iosevka Nerd Font Mono:style=Regular:style=Medium:size=23:antialias=true:hinting=true,\
xft:Unifont:style=Medium:size=21
! Bolt
URxvt.boldFont: xft:Iosevka Nerd Font Mono:style=Bold:size=23:antialias=true:hinting=true,\
xft:Unifont:style=Medium:size=21
! Italic
URxvt.italicFont: xft:Iosevka Nerd Font Mono:style=Light,Regular:size=23:antialias=true:hinting=true,\
xft:Unifont:style=Medium:size=21

Xft*antialias         : true
Xft.hinting           : true
Xft.hintstyle         : hintfull
Xft.autohint          : true
Xft*dpi               : 96

URxvt*letterSpace: +1
!URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 92x24
URxvt.internalBorder: 4
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.depth: 24
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.intensityStyles: false

URxvt.perl-ext-common: default,matcher
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

! True Transparency
!URxvt.depth: 32
!URxvt.background: [90]#000000

! Fake Transparency
!URxvt*inheritPixmap: true
!URxvt*transparent: true
!URxvt*shading: 110
